<?xml version="1.0" encoding="UTF-8" ?>
<OpenSimDocument Version="30000">
	<ForceSet name="MoBL_ARMS_Actuators">
		<objects>
        <!-- Actuators on the base segement are called Residual actuators 
             whose forces account for the errors in the model, its kinematic
             that do not balance with experimental ground reaction forces -->
			<TorqueActuator name="rz">
				<!--Flag indicating whether the force is disabled or not. Disabled means that the force is not active in subsequent dynamics realizations.-->
				<isDisabled>false</isDisabled>
				<!--Minimum allowed value for control signal. Used primarily when solving for control values.-->
				<min_control>-4</min_control>
				<!--Maximum allowed value for control signal. Used primarily when solving for control values.-->
				<max_control>4</max_control>
				<!--Name of Body to which the torque actuator is applied.-->
				<bodyA>thorax</bodyA>
				<!--Name of Body to which the equal and opposite torque is applied.-->
				<bodyB>ground</bodyB>
				<!--Interpret axis in Ground frame if true; otherwise, body A's frame.-->
				<torque_is_global>true</torque_is_global>
				<!--Fixed direction about which torque is applied, in Ground or body A frame depending on 'torque_is_global' property.-->
				<axis>0 0 1</axis>
				<!--The maximum torque produced by this actuator when fully activated.-->
				<optimal_force>25</optimal_force>
			</TorqueActuator>
			<TorqueActuator name="rx">
				<!--Flag indicating whether the force is disabled or not. Disabled means that the force is not active in subsequent dynamics realizations.-->
				<isDisabled>false</isDisabled>
				<!--Minimum allowed value for control signal. Used primarily when solving for control values.-->
				<min_control>-4</min_control>
				<!--Maximum allowed value for control signal. Used primarily when solving for control values.-->
				<max_control>4</max_control>
				<!--Name of Body to which the torque actuator is applied.-->
				<bodyA>thorax</bodyA>
				<!--Name of Body to which the equal and opposite torque is applied.-->
				<bodyB>ground</bodyB>
				<!--Interpret axis in Ground frame if true; otherwise, body A's frame.-->
				<torque_is_global>true</torque_is_global>
				<!--Fixed direction about which torque is applied, in Ground or body A frame depending on 'torque_is_global' property.-->
				<axis>1 0 0</axis>
				<!--The maximum torque produced by this actuator when fully activated. 35-->
				<optimal_force>35</optimal_force>
			</TorqueActuator>
			<TorqueActuator name="ry">
				<!--Flag indicating whether the force is disabled or not. Disabled means that the force is not active in subsequent dynamics realizations.-->
				<isDisabled>false</isDisabled>
				<!--Minimum allowed value for control signal. Used primarily when solving for control values.-->
				<min_control>-4</min_control>
				<!--Maximum allowed value for control signal. Used primarily when solving for control values.-->
				<max_control>4</max_control>
				<!--Name of Body to which the torque actuator is applied.-->
				<bodyA>thorax</bodyA>
				<!--Name of Body to which the equal and opposite torque is applied.-->
				<bodyB>ground</bodyB>
				<!--Interpret axis in Ground frame if true; otherwise, body A's frame.-->
				<torque_is_global>true</torque_is_global>
				<!--Fixed direction about which torque is applied, in Ground or body A frame depending on 'torque_is_global' property.-->
				<axis>0 1 0</axis>
				<!--The maximum torque produced by this actuator when fully activated.-->
				<optimal_force>15</optimal_force>
			</TorqueActuator>
		</objects>
		<groups />
	</ForceSet>
</OpenSimDocument>
